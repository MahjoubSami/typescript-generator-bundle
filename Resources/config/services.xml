<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="irontec.type_script_generator.class">Irontec\TypeScriptGeneratorBundle\Command\GenerateInterfaceCommand</parameter>
    </parameters>

    <services>
        <service id="irontec.type_script_generator" class="%irontec.type_script_generator.class%">
            <argument type="service" id="parameter_bag" />

            <tag name="console.command" />
        </service>
    </services>
</container>
